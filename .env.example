# Convex deployment URL (set after running `npx convex dev --configure=new`)
VITE_CONVEX_URL=

# OpenAI API key (for GPT-4o agent and GPT-4o-mini simulators)
OPENAI_API_KEY=

# OpenRouter API key (for Claude Sonnet 4.5 agent)
OPENROUTER_API_KEY=

# Groq API key (for Groq-hosted models: Llama, Mixtral, etc.)
GROQ_API_KEY=

# Galileo API key (for post-run evaluation scoring)
GALILEO_API_KEY=

# Optional: token used by Convex HTTP ingest endpoint for Python SDK sync
# Set this in Convex env as PYTHON_INGEST_TOKEN, and use same value in
# python/.env as CONVEX_PYTHON_INGEST_TOKEN.
# PYTHON_INGEST_TOKEN=

# Clerk publishable key (for authentication)
VITE_CLERK_PUBLISHABLE_KEY=

# Clerk JWT issuer domain (set in Convex dashboard as environment variable)
# CLERK_JWT_ISSUER_DOMAIN=https://your-instance.clerk.accounts.dev

# Maximum models enabled simultaneously (default: 10, minimum: 1)
# Frontend: set VITE_MAX_ENABLED_MODELS in your .env file
# Backend:  set MAX_ENABLED_MODELS in Convex dashboard (Settings > Environment Variables)
VITE_MAX_ENABLED_MODELS=10
